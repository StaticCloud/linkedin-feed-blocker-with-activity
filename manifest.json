{
  "content_scripts": [
    {
      "css": [
        "linkedin-feed-blocker.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ],
  "description": "A custom fork of the LinkedIn feed blocker that keeps activity feeds intact.",
  "icons": {
    "16": "img/ghost-16.png",
    "128": "img/ghost-128.png",
    "512": "img/ghost-512.png"
  },
  "manifest_version": 2,
  "name": "LinkedIn Feed Blocker with User Activity",
  "version": "0.0.2"
}
